Class = state -> table ->
    args ->
        key -> arg -> table key args state arg =>

Human = Class { .x:(box 0) } {
    .walkLeft: () -> obj -> () -> obj.x.apply(-1),
    .walkRight: () -> obj -> () -> obj.x.apply 1,
    .status: args -> obj -> () -> println("x = " + (obj.x.get + args.x) + "y = " + (args.y)),
} =>

h = Human() => [
  h.walkLeft(),
  h.walkRight(),
  h.walkRight(),
  h.status(),
]